        <div class="page-wrapper bg-red p-t-180 p-b-100 font-robo">
            <div class="wrapper wrapper--w960">
                <div class="card card-2">

                    <div class="card-body">
                        <h2 class="title">Hola! Ingresá con tu usuario</h2>
                        <div class="contenedorForm">
                            <form form action="/loggedIn" method="POST">
                               
                                <div class="input-group">
                                    <input class="input--style-2" id="username" type="text" placeholder="Nombre de Usuario" name="username" required>
                                </div>
                                
                                <div class="input-group">
                                    <input class="input--style-2" id="password" type="password" placeholder="Contraseña" name="password" required>
                                </div>

                                <div class="p-t-30">
                                    <button class="btn btn--radius btn--green" type="submit">Enviar</button>
                                </div>
                            </form>
                        </div>

                        <div class="errores" style="color:red"></div>
                    </div>
                </div>
            </div>
        </div>
    </main>





    <!-- FOOTER-->
    <footer class="footerPartial">
        <%- include('../partials/footer') %>
    </footer>

</body>
</html>