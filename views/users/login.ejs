<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ingresa a tu cuenta</title>
    
    <link rel="stylesheet" href="/stylesheets/signup.css">
</head>
<body>
    <!-- HEADER-->
    <section class="headerPartial"
      <%- include('../partials/header') %>
    </section>

    <main class="formularioLogin">
        <!-- <div class="title"></div>
        
            <div class="inputsLogin">
                
                <div class="completar"> <input type="text" name="username" placeholder="Nombre de usuario" required> </div>
                <div class="completar"><input type="password" name="password" placeholder="Contraseña" required></div>
                
